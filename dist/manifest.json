{
    "manifest_version": 3,
    "name": "Message Templater",
    "description": "Chrome Extension for sending template messages",
    "version": "0.1.0",
    "action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },
    "background": {},
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ]
}
